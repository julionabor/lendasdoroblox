{
	"version": 2,
	"builds": [
		{
			"src": "backend/index.js",
			"use": "@vercel/node"
		},
		{
			"src": "front/package.json",
			"use": "@vercel/static-build",
			"config": {
				"distDir": "build"
			}
		}
	],
	"routes": [
		{
			"src": "/backend/(.*)",
			"dest": "/api/index.js"
		},
		{
			"src": "^/static/(.*)",
			"dest": "/client/static/$1"
		},
		{
			"src": "/(.*)\\.png",
			"dest": "/client/$1.png"
		},
		{
			"src": "/(.*)",
			"dest": "/client/index.html"
		}
	]
}
